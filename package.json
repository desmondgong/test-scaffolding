{
  "name": "test-scaffolding",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules lib src/tests/out",
    "build": "cross-env NODE_ENV=production yarn build:app && yarn build:server",
    "build:app": "webpack --config=webpack.config.js",
    "build:server": "babel ./server --out-dir ./lib/server --copy-files",
    "test": "NODE_ENV=test jest",
    "start": "cross-env NODE_ENV=production node ./lib/server/server.js",
    "start:dev": "cross-env NODE_ENV=development babel-node ./server/server.js",
    "check:lint": "eslint -f html --ext .js,.jsx,.ts,.tsx src/ -o src/tests/out/lint-report.html"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/styles": "^4.10.0",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/polyfill": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@typescript-eslint/parser": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "fetch-mock": "^9.10.7",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4",
    "ts-loader": "^8.0.4",
    "ts-jest": "^26.4.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/desmondgong/lunch-dilemma"
  }
}
